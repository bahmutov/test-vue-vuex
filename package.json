{
  "name": "test-vue-vuex",
  "version": "1.0.0",
  "description": "Testing Vue + Vuex + REST TodoMVC using Cypress.",
  "main": "app.js",
  "scripts": {
    "test": "cypress open",
    "start": "json-server --static . data.json --middlewares node_modules/json-server-reset",
